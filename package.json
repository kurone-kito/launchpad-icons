{
  "name": "@kurone-kito/launchpad-icons",
  "version": "0.1.0",
  "private": true,
  "description": "the icons VPM asset like the VRChat LaunchPad for the VRChat and Web",
  "keywords": [
    "Icons",
    "LaunchPad",
    "VRChat"
  ],
  "homepage": "https://github.com/kurone-kito/yarn-project-boilerplate#readme",
  "bugs": "https://github.com/kurone-kito/yarn-project-boilerplate/issues",
  "repository": "github:kurone-kito/yarn-project-boilerplate",
  "license": "CC BY-NC 4.0",
  "author": "kurone-kito <krone@kit.black> (https://kit.black/)",
  "type": "module",
  "files": [],
  "scripts": {
    "build": "vinxi build",
    "clean": "rimraf -g .eslintcache .output .vinxi \"*.tgz\" \"*.tsbuildinfo\" \"node_modules/.cache/**/*\"",
    "dev": "pnpm run \"/dev:.*/\"",
    "dev:browser": "delay 5 && open-cli \"http://localhost:3000/launchpad-icons/\"",
    "dev:vinxi": "vinxi dev",
    "postinstall": "husky",
    "lint": "pnpm run \"/lint:.*:check/\"",
    "lint:cspell:check": "cspell lint --no-progress --show-suggestions -u \"./**/*\"",
    "lint:eslint:check": "eslint --cache --cache-location node_modules/.cache/eslint/ --cache-strategy=content -f codeframe \"./**/*\"",
    "lint:eslint:fix": "pnpm run lint:eslint:check --fix",
    "lint:fix": "pnpm run \"/lint:.*:fix/\"",
    "lint:oxlint:check": "oxlint .",
    "lint:oxlint:fix": "oxlint --fix .",
    "lint:prettier:check": "pnpm run prettier -cu",
    "lint:prettier:fix": "pnpm run prettier -uw",
    "prettier": "prettier --cache --log-level=warn \"./**/*\"",
    "serve": "vinxi serve --dir .output/public --base /launchpad-icons",
    "start": "vinxi start",
    "test": "pnpm run lint"
  },
  "dependencies": {
    "@solidjs/router": "^0.13.5",
    "@solidjs/start": "^1.0.1",
    "solid-js": "^1.8.17"
  },
  "prettier": "@kurone-kito/prettier-config",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cspell/cspell-types": "^8.8.3",
    "@cspell/eslint-plugin": "^8.8.3",
    "@kurone-kito/commitlint-config": "^0.11.0",
    "@kurone-kito/cspell-config": "^0.11.0",
    "@kurone-kito/eslint-config-base": "^0.11.0",
    "@kurone-kito/lint-staged-config": "^0.11.0",
    "@kurone-kito/prettier-config": "^0.11.0",
    "@kurone-kito/typescript-config": "^0.11.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "autoprefixer": "^10.4.19",
    "cspell": "^8.8.3",
    "delay-cli": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-editorconfig": "^4.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.7",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-markdownlint": "^0.6.0",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-oxlint": "^0.4.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "open-cli": "^8.0.0",
    "oxlint": "^0.4.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.5",
    "typescript-eslint-language-service": "^5.0.5",
    "vinxi": "^0.3.11"
  },
  "packageManager": "pnpm@9.1.4+sha512.9df9cf27c91715646c7d675d1c9c8e41f6fce88246f1318c1aa6a1ed1aeb3c4f032fcdf4ba63cc69c4fe6d634279176b5358727d8f2cc1e65b65f43ce2f8bfb0",
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  }
}
