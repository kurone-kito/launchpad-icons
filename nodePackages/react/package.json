    "prebuild:code": "liBuilder react",
    "prebuild:license": "cpy --flat ../../LICENSE . && cpy --flat ../../LICENSE.*.txt .",
    "prestart": "pnpm run prebuild",
    "start": "tsc -w",
    "test": "vitest run"
  },
  "devDependencies": {
    "@kurone-kito/launchpad-icons-builder": "workspace:^",
