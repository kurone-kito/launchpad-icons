    "jsdom": "^26.0.0",
    "react": "^18.3.1",
    "rimraf": "^6.0.1",
    "typedoc": "^0.28.2",
    "typedoc-plugin-markdown": "~4.6.1",
    "typescript": "~5.8.3",
    "vitest": "^3.1.1"
